#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
int count_minority(int n, int num[]) {
	int cnt = 0;
	int i, j;

	for (i = 0; i < n; i++) {
		if (num[i] == 1) continue;
		for (j = 2; j < num[i]; j++) {
			if (num[i] % j == 0) break;
		}
		if (j == num[i]) cnt++;
	}
	return cnt;
}
int main(void) {
	int n;
	int* numbers;

	scanf("%d", &n);
	numbers = (int*)malloc(sizeof(int) * n);

	for (int i = 0; i < n; i++) {
		scanf("%d", &numbers[i]);
	}

	printf("%d", count_minority(n, numbers));
}